{
  "repository": "joachimdalen/auto-state",
  "changelogTitle": {
    "size": "h1",
    "format": "Changelog"
  },
  "releaseTitleFormat": {
    "size": "h2",
    "format": "{{version}} ({{publishDate}})"
  },
  "moduleTitleFormat": {
    "size": "h4",
    "format": "`{{name}}@{{version}}`"
  },
  "taskMapping": {},
  "sectionSplitter": "---",
  "tagSize": "h3",
  "tagMapping": {
    "feature": ":rocket: Features",
    "fix": ":bug: Fixes",
    "tests": ":test_tube: Tests",
    "other": ":speech_balloon: Other",
    "docs": ":memo: Documentation"
  },
  "attributionTitleFormat": {
    "size": "h2",
    "format": ":star2: Contributors"
  },
  "attributionSubTitle": {
    "format": "Thank you to the following for contributing to the latest release"
  },
  "knownAuthors": [],
  "useDescriptiveIssues": true,
  "replaceEmojis": {
    "tags": true,
    "changelogTitle": true,
    "releaseTitle": true,
    "moduleTitle": true,
    "attributionTitle": true,
    "attributionSubTitle": true,
    "githubIssues": false,
    "githubPullRequests": false,
    "notes": true,
    "summary": true
  }
}